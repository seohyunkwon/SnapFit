spring.application.name=SnapFit

# dataSource
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
# 빈칸에 데이터 베이스의 이름 작성
spring.datasource.url=jdbc:mysql://localhost:3306/spring_quest?serverTimezone=UTC
spring.datasource.username=ssafy
spring.datasource.password=ssafy

# mybatis
mybatis.mapper-locations=classpath*:mappers/*.xml
# dto가 들어있는 패키지명을 작성해 타입을 매핑해준다.
mybatis.type-aliases-package=com.ssafy.spring.model.dto
mybatis.configuration.map-underscore-to-camel-case=true
